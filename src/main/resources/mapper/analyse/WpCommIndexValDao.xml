<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.dfjinxin.modules.analyse.dao.WpCommIndexValDao">

	<!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.dfjinxin.modules.analyse.entity.WpCommIndexValEntity" id="wpCommIndexValMap">
        <result property="indexI" column="index_i"/>
        <result property="commId" column="comm_id"/>
        <result property="indexName" column="index_name"/>
        <result property="indexType" column="index_type"/>
        <result property="statAreaId" column="stat_area_id"/>
        <result property="statAreaName" column="stat_area_name"/>
        <result property="statFrequency" column="stat_frequency"/>
        <result property="indexVal" column="index_val"/>
        <result property="indexUnit" column="index_unit"/>
        <result property="dataTime" column="data_time"/>
        <result property="dataSource" column="data_source"/>
    </resultMap>

    <select id="queryIndexTypeByCommId" resultType="java.util.HashMap">
        SELECT t1.index_type from wp_comm_index_val t1
        where t1.comm_id=#{commId}
        group by t1.index_type
        order by t1.index_type
    </select>
</mapper>
