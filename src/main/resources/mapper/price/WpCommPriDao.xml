<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.dfjinxin.modules.price.dao.WpCommPriDao">

    <!-- 可根据自己的需求，是否要使用 -->
    <resultMap type="io.dfjinxin.modules.price.entity.WpCommPriEntity" id="wpCommPriMap">
        <result property="seqId" column="seq_id"/>
        <result property="commId" column="comm_id"/>
        <result property="commName" column="comm_name"/>
        <result property="areaName" column="area_name"/>
        <result property="areaId" column="area_id"/>
        <result property="countryName" column="country_name"/>
        <result property="countryId" column="country_id"/>
        <result property="statFreq" column="stat_freq"/>
        <result property="currency" column="currency"/>
        <result property="priType" column="pri_type"/>
        <result property="priToday" column="pri_today"/>
        <result property="priUnit" column="pri_unit"/>
        <result property="dataTime" column="data_time"/>
        <result property="dataSource" column="data_source"/>
    </resultMap>

    <select id="selectLastPrice" resultType="io.dfjinxin.modules.price.entity.WpCommPriEntity">
        select wcp.* from wp_comm_pri wcp where comm_id =#{commId} order by data_time desc limit 0,1;
    </select>


</mapper>
